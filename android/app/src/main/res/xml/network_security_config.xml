<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />
            <!-- Add your custom certificate here -->
            <certificates src="@raw/server" />
        </trust-anchors>
    </base-config>
</network-security-config>